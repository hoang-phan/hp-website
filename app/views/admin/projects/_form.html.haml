= form_for [:admin, resource] do |f|
  .form-group
    = f.label :name
    = f.text_field :name, class: 'form-control'
  .form-group
    = f.label :start_month
    = f.number_field :start_month, class: 'form-control'
  .form-group
    = f.label :start_year
    = f.number_field :start_year, class: 'form-control'
  %p Members:
  = collection_check_boxes(:project, :member_ids, Member.all, :id, :name) do |b|
    .form-group
      = b.check_box
      = b.label
  .form-group
    = f.submit class: 'btn btn-default'
